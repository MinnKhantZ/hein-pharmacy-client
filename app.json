{
  "expo": {
    "name": "Hein Pharmacy",
    "slug": "hein-pharmacy",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "heinpharmacy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth to connect to thermal printers for receipt printing.",
        "NSBluetoothPeripheralUsageDescription": "This app needs Bluetooth to connect to thermal printers for receipt printing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffe883",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.minkhantzaw.heinpharmacy",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./plugins/withAndroidBluetoothPermissions",
      "expo-updates",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffe883",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/noti-icon.png",
          "color": "#ffe883",
          "androidMode": "default"
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-secure-store"
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 5000,
      "url": "https://u.expo.dev/fbad1d52-46d0-45ca-afdc-2939a8742dc5"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fbad1d52-46d0-45ca-afdc-2939a8742dc5"
      }
    },
    "owner": "minkhantzaw",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
